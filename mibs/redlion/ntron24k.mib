--  ----------------------------------------------------------------------------------

--  ----------------------------------------------------------------------------------
ntronRstpBridge   OBJECT IDENTIFIER     ::= {ntronRstpConfig 1}
--  ----------------------------------------------------------------------------------
ntronRstpBridgeWebStatus    OBJECT-TYPE
    SYNTAX                  INTEGER
                            {
                                disabled    (3),
                                forceStp    (5),
                                rstp        (7)
                            }
    MAX-ACCESS              read-write
    STATUS                  current
    DESCRIPTION             "This is the configured RSTP protocol on this bridge."
    ::= { ntronRstpBridge 1 }

ntronRstpBridgeVlanId           OBJECT-TYPE
    SYNTAX                      Integer32(1..4094)
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "RSTP will operate on all ports of the indicated VLAN"
    ::= {ntronRstpBridge 2}

ntronRstpBridgeBridgePriorityStep       OBJECT-TYPE
    SYNTAX                      INTEGER
                                {
                                    priority0       (1),
                                    priority4096    (2),
                                    priority8192    (3),
                                    priority12288   (4),
                                    priority16384   (5),
                                    priority20480   (6),
                                    priority24576   (7),
                                    priority28672   (8),
                                    priority32768   (9),
                                    priority36864   (10),
                                    priority40960   (11),
                                    priority45056   (12),
                                    priority49152   (13),
                                    priority53248   (14),
                                    priority57344   (15),
                                    priority61440   (16)
                                }
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "The Priority of this Bridge."
    ::= {ntronRstpBridge 3}

ntronRstpBridgeMacAddress       OBJECT-TYPE
    SYNTAX                      DisplayString(SIZE(0..255))
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION                 "This object will return the MAC address of this Bridge."
    ::= { ntronRstpBridge 4 }

ntronRstpBridgeHelloTime        OBJECT-TYPE
    SYNTAX                      Integer32(1..10)
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "This is the time interval, in seconds, between the transmission
                                of Configuration BPDUs by Designated Ports."
    ::= {ntronRstpBridge 5}

ntronRstpBridgeFowardDelay      OBJECT-TYPE
    SYNTAX                      Integer32(4..30)
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "This is the time spent, in seconds, by legacy STP Bridges in transitioning
                                Root and Designated Ports to Forwarding. This delays Port transitions until
                                other bridges have received spanning tree information."
    ::= {ntronRstpBridge 6}

ntronRstpBridgeMaxAge   OBJECT-TYPE
    SYNTAX                      Integer32(6..40)
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "For RSTP: this is the maximum hops allowed for any protocol message
                                before it is discarded and no longer retransmitted.
                                For STP: this is the maximum time, in seconds, that a bridge will wait
                                for Configuration BPDUs before deciding it is no longer connected to
                                the root bridge."
    ::= {ntronRstpBridge 7}

ntronRstpBridgeTxHoldCount      OBJECT-TYPE
    SYNTAX                      Integer32(1..10)
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "The maximum number of Configuration BPDUs that
                                can be transmitted in one second, from a port
                                on this Bridge before transmission is throttled. "
    ::= {ntronRstpBridge 8}

ntronRstpBridgeTopologyChanges  OBJECT-TYPE
    SYNTAX                      Integer32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION                 "The number of RSTP topology changes since the switch has been powered
                                on or rebooted"
    ::= {ntronRstpBridge 9}

ntronRstpCurrentStatus  OBJECT-TYPE
    SYNTAX                  INTEGER
                            {
                                disabled    (3),
                                forceStp    (5),
                                rstp        (7)
                            }
    MAX-ACCESS              read-write
    STATUS                  current
    DESCRIPTION             "This is the current RSTP protocol on this bridge."
    ::= { ntronRstpBridge 10 }

ntronRstpBridgeBpduFlooding  OBJECT-TYPE
    SYNTAX                      INTEGER
                                {
                                    block   (1),
                                    flood   (2)
                                }
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION                 "This value allows the flooding or blocking
                                 of BPDUs to other ports when the global
                                 spanning tree is disabled or when spanning
                                 tree is disabled on a specific port.
                                 If flood is selected, BPDUs are flooded to
                                 all other ports based on the VLAN assignment.
                                 The options are block(1) or flood(2).
                                 The default is flood(2)."
    ::= {ntronRstpBridge 11}

ntronRstpPortsTable             OBJECT-TYPE
    SYNTAX                      SEQUENCE OF NtronRstpPorts
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION                 "RSTP Ports table"
    ::= { ntronRstpConfig 2 }

ntronRstpPortsPort              OBJECT-TYPE
    SYNTAX                      NtronRstpPorts
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION                 "An entry in the ntronRstpPortsTable."
    INDEX                       {ntronRstpPortsPortPortNumberId }
    ::= {ntronRstpPortsTable 1 }

NtronRstpPorts ::= SEQUENCE {
    ntronRstpPortsPortNumber                DisplayString,
    ntronRstpPortsPortVlanId                Integer32,
    ntronRstpPortsPortState                 INTEGER,
    ntronRstpPortPortStpBpdu                INTEGER,
    ntronRstpPortsPortPathCost              Integer32,
    ntronRstpPortsPortCurrentPathCost       Integer32,
    ntronRstpPortsPortPortPriorityStep      INTEGER,
